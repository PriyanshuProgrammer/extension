{
  "name": "Example Gmail Extension",
  "description": "Example extension showing use of the Streak Gmail SDK",
  "version": "0.1",
  "action": {
    "default_popup": "index.html"
  },
  "content_scripts": [
    {
      "matches": ["https://mail.google.com/*"],
      "js": ["contentScript.js"],
      "run_at": "document_end"
    }
  ],
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["scripting", "http://localhost:3000/*"],
  "host_permissions": ["https://mail.google.com/", "http://localhost:3000/*"],
  "manifest_version": 3,
  "web_accessible_resources": [
    {
      "resources": ["*.map"],
      "matches": ["https://mail.google.com/*"]
    }
  ]
}
